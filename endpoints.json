{
  "endpoints": {
    "GET /api": {
      "description": "Serves up a json representation of all the available endpoints of the API backendDevPractise"
    },
    "GET /api/topics": {
      "description": "Serves an array of all topics",
      "queries": "n/a",
      "exampleResponse": {
        "topics": [
          {
            "slug": "coding",
            "description": "Code is love, code is life"
          },
          {
            "slug": "football",
            "description": "FOOTIE!"
          },
          {
            "slug": "cooking",
            "description": "Hey good looking, what you got cooking?"
          }
        ]
      }
    },
    "GET /api/users/:username": {
      "description": "Serves a user object when a parametric of username given",
      "queries": "n/a",
      "exampleResponse": {
        "user": [
          {
            "username": "jessjelly",
            "avatar_url": "https://s-media-cache-ak0.pinimg.com/564x/39/62/ec/3962eca164e60cf46f979c1f57d4078b.jpg",
            "name": "Jess Jelly"
          }
        ]
      }
    },
    "GET /api/articles/:article_id": {
      "description": "Serves an article object when a parametric of article_id given",
      "queries": "n/a",
      "exampleResponse": {
        "article": {
          "author": "rogersop",
          "title": "Jedi returns",
          "article_id": 1,
          "body": "We are returning home",
          "topic": "Star fan",
          "created_at": "2016-07-08T23:00:00.000Z",
          "votes": 5,
          "comment_count": 2
        }
      }
    },
    "PATCH /api/articles/:article_id": {
      "description": "Returns an article object with an update of vote when a patch of vote sent",
      "queries": "n/a",
      "patch": {
        "inc_votes": 1
      },
      "exampleResponse": {
        "article": {
          "author": "sop",
          "title": "Who returns",
          "article_id": 1,
          "body": "We are home",
          "topic": "Trek fan",
          "created_at": "2016-07-08T23:00:00.000Z",
          "votes": 5,
          "comment_count": 0
        }
      }
    },
    "POST /api/articles/:article_id/comments": {
      "description": "When data is posted, it returns the newly created comment object",
      "queries": "n/a",
      "post": {
        "username": "rogersop",
        "body": "Hey ya'll"
      },
      "exampleResponse": {
        "comment": {
          "author": "rogersop",
          "body": "Hey ya'll"
        }
      }
    },
    "GET /api/articles/:article_id/comments": {
      "description": "Returns an array of comments object",
      "queries": ["sort_by", "order_by"],
      "exampleResponse": {
        "comments": [
          {
            "comment_id": 1,
            "body": "Hey ya'll",
            "created_at": "2016-07-08T23:00:00.000Z",
            "votes": 2,
            "author": "roger"
          },
          {
            "comment_id": 2,
            "body": "Hey",
            "created_at": "2016-07-08T23:00:00.000Z",
            "votes": 0,
            "author": "jane"
          }
        ]
      }
    },
    "GET /api/articles": {
      "description": "Serves an array of articles objects",
      "queries": ["author", "topic", "sort_by", "order_by"],
      "exampleResponse": {
        "articles": [
          {
            "author": "rogersop",
            "title": "Jedi returns",
            "article_id": 1,
            "body": "We are returning home",
            "topic": "Star fan",
            "created_at": "2016-07-08T23:00:00.000Z",
            "votes": 5,
            "comment_count": 2
          },
          {
            "author": "sop",
            "title": "Who returns",
            "article_id": 2,
            "body": "We are home",
            "topic": "Trek fan",
            "created_at": "2016-07-08T23:00:00.000Z",
            "votes": 1,
            "comment_count": 0
          }
        ]
      }
    },
    "PATCH /api/comments/:comment_id": {
      "description": "Patches and then serves a single comments object with an updated of its `votes` property value",
      "queries": "n/a",
      "patch": { "inc_votes": 1 },
      "exampleResponse": {
        "comment": {
          "comment_id": 1,
          "author": "butter_bridge",
          "article_id": 9,
          "votes": 18,
          "created_at": "2017-11-22T12:36:03.000Z",
          "body": "Oh, I've got compassion running out of my nose, pal! I'm the Sultan of Sentiment!"
        }
      }
    },
    "DELETE /api/comments/:comment_id": {
      "description": "Deletes a specific comment and serves nothing to see",
      "queries": "n/a",
      "exampleResponse": "nil"
    }
  }
}
